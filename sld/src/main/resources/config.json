{
    "Config": {
        "SolrServer": "http://localhost:8983/solr/bsrb",
        "Output": "none",
        "Wildcard": "|?|",
        "DefaultImg": "none",
        "CorticalThreshold": -1,
        "LinksThreshold": -1,
        "SynNumber": -1,
        "Stopwords": [],
        "Repositories": [
            {
                "Name": "lbPersons",
                "ApplyFilter": false,
                "FilterURI": "",
                "DateBoost": false,
                "Date": "",
                "TwoSteps": false,
                "Query2": "",
                "Endpoint": "http://localhost:7200/repositories/library",
                "MainClass": "http://xmlns.com/foaf/0.1/Person",
                "Query": "select ?d (count( distinct ?u) as ?key) { bind (<|?|> as ?p ) . ?p ?mock ?u . ?u <http://purl.org/dc/terms/subject> ?d . } group by ?d Order by DESC (?key) limit 30"
            }, {
                "Name": "lbDocuments",
                "ApplyFilter": false,
                "FilterURI": "",
                "DateBoost": false,
                "Date": "",
                "TwoSteps": false,
                "Query2": "",
                "Endpoint": "http://localhost:7200/repositories/library",
                "MainClass": "http://purl.org/ontology/bibo/Document",
                "Query": "select distinct ?d {bind (<|?|> as ?u ) .  { ?u  <http://purl.org/dc/terms/subject> ?d . } union { ?u  <http://schema.org/mentions> ?m . ?m  <http://purl.org/saws/ontology#refersTo> ?d . }}"
            }, {
                "Name": "lbCollections",
                "ApplyFilter": false,
                "FilterURI": "",
                "DateBoost": false,
                "Date": "",
                "TwoSteps": false,
                "Query2": "",
                "Endpoint": "http://localhost:7200/repositories/library",
                "MainClass": "http://purl.org/ontology/bibo/Collection",
                "Query": "select ?d (count( distinct ?u) as ?key) { bind (<|?|> as ?p ) . ?u ?mock ?p . ?u <http://purl.org/dc/terms/subject> ?d . } group by ?d Order by DESC (?key) limit 30"
            }
        ]
    }
}