<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <listener>
        <listener-class>ec.edu.ucuenca.dcc.sld.ContextListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>Query</servlet-name>
        <servlet-class>ec.edu.ucuenca.dcc.sld.Query</servlet-class>
        <init-param>
            <param-name>uri</param-name>
            <param-value>none</param-value>
        </init-param>
    </servlet>

    <servlet>
        <servlet-name>sparql</servlet-name>
        <servlet-class>ec.edu.ucuenca.dcc.sld.sparql</servlet-class>
        <init-param>
            <param-name>uri</param-name>
            <param-value>none</param-value>
        </init-param>
    </servlet>

    <servlet-mapping>
        <servlet-name>sparql</servlet-name>
        <url-pattern>/sparql</url-pattern>
    </servlet-mapping>
</web-app>
